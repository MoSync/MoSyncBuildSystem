<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <logRotator>
    <daysToKeep>7</daysToKeep>
    <numToKeep>30</numToKeep>
    <artifactDaysToKeep>7</artifactDaysToKeep>
    <artifactNumToKeep>30</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>TIMESTAMP</name>
          <description></description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <assignedNode>packingwin</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>Prepare_MoSync_Source_Tarball</projectName>
      <filter>MoSyncSrc.tar.bz2</filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.StatusBuildSelector"/>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>Build_Windows_Runtimes</projectName>
      <filter></filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.PermalinkBuildSelector">
        <id>lastBuild</id>
      </selector>
      <optional>true</optional>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>Build_Libs_Tools_Examples_On_Windows</projectName>
      <filter></filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.PermalinkBuildSelector">
        <id>lastBuild</id>
      </selector>
      <optional>true</optional>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>Build_Eclipse</projectName>
      <filter>installer_splash.bmp, MoSync-win32.win32.x86-trimmed.zip</filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.PermalinkBuildSelector">
        <id>lastBuild</id>
      </selector>
      <optional>true</optional>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>Build_Linux_Runtimes</projectName>
      <filter></filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.PermalinkBuildSelector">
        <id>lastBuild</id>
      </selector>
      <optional>true</optional>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>Build_iOS_Runtime</projectName>
      <filter></filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.PermalinkBuildSelector">
        <id>lastBuild</id>
      </selector>
      <optional>true</optional>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>Build_WP7_Runtime</projectName>
      <filter></filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.PermalinkBuildSelector">
        <id>lastBuild</id>
      </selector>
      <optional>true</optional>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>Build_VS2010_Libs_MoRE</projectName>
      <filter></filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.PermalinkBuildSelector">
        <id>lastBuild</id>
      </selector>
      <optional>true</optional>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.plugins.copyartifact.CopyArtifact>
      <projectName>Build_HTML5_Documentation</projectName>
      <filter></filter>
      <target></target>
      <selector class="hudson.plugins.copyartifact.PermalinkBuildSelector">
        <id>lastBuild</id>
      </selector>
      <optional>true</optional>
    </hudson.plugins.copyartifact.CopyArtifact>
    <hudson.tasks.BatchFile>
      <command>mkdir mosync-trunk
7z x MoSyncSrc.tar.bz2
7z x MoSyncSrc.tar -omosync-trunk

mkdir MoSyncWinPackage
7z x *.zip -oMoSyncWinPackage -aoa

cd MoSyncWinPackage
mkdir eclipse
cd ..
cp -r MoSyncWinPackage/mosync/* MoSyncWinPackage/eclipse/
rm -r MoSyncWinPackage/mosync

mv mosync-trunk/tools/ReleasePackageBuild/InstallerResources/* MoSyncWinPackage/
mv mosync-trunk/tools/ReleasePackageBuild/build_package_tools/mosync_prerequisites/* MoSyncWinPackage/
cp mosync-trunk/buildInfo.txt MoSyncWinPackage/bin/
cp mosync-trunk/tools/ReleasePackageBuild/build_package_tools/mosync_docs/licenses/mosync-license.txt MoSyncWinPackage/
cd MoSyncWinPackage
rename mosync-license.txt license.txt
cd ..

cp installer_splash.bmp MoSyncWinPackage/

mosync-trunk\tools\ReleasePackageBuild\build_package_tools\NSIS\makensis MoSyncWinPackage\MoSync.nsi
if %errorlevel% neq 0 exit /b %errorlevel%
mv MoSyncWinPackage/MoSyncSetup.exe MoSyncWindows-%TIMESTAMP%.exe</command>
    </hudson.tasks.BatchFile>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>MoSyncWindows-*.exe</artifacts>
      <latestOnly>false</latestOnly>
    </hudson.tasks.ArtifactArchiver>
    <hudson.tasks.Fingerprinter>
      <targets></targets>
      <recordBuildArtifacts>true</recordBuildArtifacts>
    </hudson.tasks.Fingerprinter>
  </publishers>
  <buildWrappers>
    <hudson.plugins.ws__cleanup.PreBuildCleanup>
      <deleteDirs>false</deleteDirs>
    </hudson.plugins.ws__cleanup.PreBuildCleanup>
  </buildWrappers>
</project>